# check the domain else we need to skip this (done in wls_access.rb)
real_domain='<%= domain %>'

name = '<%= provider_name%>'

edit()
startEdit()

try:
    cd('/ForeignJNDIProviders')
    cmo.destroyForeignJNDIProvider(getMBean('/ForeignJNDIProviders/' + name))

    save()
    activate()
    report_back_success()

except:
    report_back_error()
