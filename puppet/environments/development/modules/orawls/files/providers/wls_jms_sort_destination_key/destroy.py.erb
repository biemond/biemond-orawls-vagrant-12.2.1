# check the domain else we need to skip this (done in wls_access.rb)
real_domain='<%= domain %>'


name                     = '<%= key_name %>'
jmsmodule                = '<%= jmsmodule %>'
edit()
startEdit()

try:

  cd('/')
  cd('/JMSSystemResources/'+jmsmodule+'/JMSResource/'+jmsmodule)
  cmo.destroyDestinationKey(getMBean('/JMSSystemResources/'+jmsmodule+'/JMSResource/'+jmsmodule+'/DestinationKeys/'+name))

  save()
  activate()
  report_back_success()

except:
  print "Unexpected error:", sys.exc_info()[0]
  undo('true','y')
  stopEdit('y')
  report_back_error()
  raise


