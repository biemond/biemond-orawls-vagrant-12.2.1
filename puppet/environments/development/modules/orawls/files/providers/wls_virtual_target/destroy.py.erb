# check the domain else we need to skip this (done in wls_access.rb)
real_domain='<%= domain %>'

name = '<%= virtual_target_name %>'

edit()
startEdit()

try:

    cd('/')
    cmo.destroyVirtualTarget(getMBean('/VirtualTargets/' + name))
    save()
    activate()
    report_back_success()

except:
    print "Unexpected error:", sys.exc_info()[0]
    undo('true','y')
    stopEdit('y')
    report_back_error()
    raise


