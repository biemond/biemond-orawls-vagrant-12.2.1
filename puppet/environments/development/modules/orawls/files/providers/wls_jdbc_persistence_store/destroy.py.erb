real_domain='<%= domain %>'



name           = '<%= jdbc_persistence_name %>'

edit()
startEdit()

try:
    cd('/JDBCStores')
    cmo.destroyJDBCStore(getMBean('/JDBCStores/'+name))
    save()
    activate()
    report_back_success()

except:
    report_back_error()
