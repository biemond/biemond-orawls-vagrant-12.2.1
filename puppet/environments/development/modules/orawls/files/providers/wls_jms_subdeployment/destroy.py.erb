# check the domain else we need to skip this (done in wls_access.rb)
real_domain='<%= domain %>'



name               = '<%= subdeployment_name %>'
jmsmodule          = '<%= jmsmodule %>'

edit()
startEdit()

try:

    cd('/')
    cd('/SystemResources/'+jmsmodule)
    cmo.destroySubDeployment(getMBean('/SystemResources/'+jmsmodule+'/SubDeployments/'+name))

    save()
    activate()
    report_back_success()

except:
    report_back_error()
